{
  "name": "icctutor-link",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "wrangler pages dev dist --d1 DB=9af27145-a8cd-48ae-b050-4544a15a9978 --port 8788",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy dist",
    "db:init": "wrangler d1 execute icctutor-db --file=./schema.sql",
    "db:migrations": "wrangler d1 migrations list icctutor-db",
    "db:migrate": "wrangler d1 migrations apply icctutor-db"
  },
  "dependencies": {
    "firebase": "^12.7.0",
    "lucide-react": "^0.263.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^4.3.4",
    "bcryptjs": "^2.4.3",
    "concurrently": "^9.1.0",
    "eslint": "^9.37.0",
    "eslint-formatter-unix": "^8.40.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "typescript": "^5.6.2",
    "vite": "^6.0.1",
    "wrangler": "^3.94.0"
  }
}
